#!/usr/bin/env python3

import subprocess


def toggle():
   output = subprocess.check_output(["amixer", "set", "Master", "toggle"])
   status = output.splitlines()[-1].rsplit(b" ", 1)[-1]
   muted = status == b"[off]"
   return not muted

def main():
   print("1" if toggle() else "0")


if __name__ == "__main__":
   main()

{
    "editor.fontFamily": "'Fira Mono', monospace",
    "window.zoomLevel": 0,
    "workbench.colorTheme": "Base16 Dark Ashes",
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never",
    "editor.autoSurround": "never",
    "editor.colorDecorators": false,
    "editor.folding": false,
    "editor.renderIndentGuides": false,
    "editor.renderLineHighlight": "none",
    "editor.occurrencesHighlight": false,
    "editor.wrappingIndent": "indent",
    "editor.minimap.maxColumn": 80,
    "editor.minimap.renderCharacters": false,
    "editor.rulers": [80],
    "editor.suggest.maxVisibleSuggestions": 8,
    "editor.suggest.showIcons": false,
    "files.enableTrash": false,
    "files.eol": "\n",
    "files.hotExit": "onExitAndWindowClose",
    "files.insertFinalNewline": true,
    "files.simpleDialog.enable": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "workbench.enableExperiments": false,
    "workbench.startupEditor": "none",
    "workbench.iconTheme": null,
    "workbench.sideBar.location": "right",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "window.menuBarVisibility": "toggle",
    "window.restoreWindows": "all",
    "extensions.ignoreRecommendations": true,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "terminal.integrated.cursorStyle": "line",
    "update.mode": "none",
    "git.showPushSuccessNotification": true,
    "html.autoClosingTags": false,
    "html.format.endWithNewline": true,
    "json.format.enable": false,
    "markdown.preview.fontFamily": "'Inter', sans-serif",
    "markdown.preview.lineHeight": 1.4,
    "diffEditor.ignoreTrimWhitespace": true,
    "editor.minimap.enabled": false,
    "diffEditor.renderSideBySide": true,
    "git.confirmSync": false,
    "workbench.colorCustomizations": {
        "[Base16 Dark Ashes]": {
            "statusBar.background": "#1d1f21",
            "statusBar.noFolderBackground": "#1d1f21",
            "statusBar.foreground": "#707880",
            "statusBar.noFolderForeground": "#707880",
            "statusBar.border": "#373b41",
            "statusBar.noFolderBorder": "#373b41"
        },
        "[Base16 Light Tomorrow]": {
            "statusBar.background": "#ffffff",
            "statusBar.noFolderBackground": "#ffffff",
            "statusBar.foreground": "#4d4d4c",
            "statusBar.noFolderForeground": "#4d4d4c",
            "statusBar.border": "#dcdcdc",
            "statusBar.noFolderBorder": "#dcdcdc"
        },
        "[Base16 Light Grayscale]": {
            "statusBar.background": "#f7f7f7",
            "statusBar.noFolderBackground": "#f7f7f7",
            "statusBar.foreground": "#727272",
            "statusBar.noFolderForeground": "#727272",
            "statusBar.border": "#d6d6d6",
            "statusBar.noFolderBorder": "#d6d6d6"
        },
        "[Base16 Dark Grayscale]": {
            "statusBar.background": "#000000",
            "statusBar.noFolderBackground": "#000000",
            "statusBar.foreground": "#949494",
            "statusBar.noFolderForeground": "#949494",
            "statusBar.border": "#2f2f2f",
            "statusBar.noFolderBorder": "#2f2f2f"
        },
    },
    "workbench.commandPalette.history": 500,
    "rust.clippy_preference": "on",
    "editor.smoothScrolling": true,
    "editor.codeLens": false,
    "editor.quickSuggestions": false,
    "scm.diffDecorations": "none",
    "editor.parameterHints.enabled": false,
    "editor.tabSize": 3,
    "editor.fontSize": 13,
    "zenMode.restore": true,
    "editor.trimAutoWhitespace": false,
    "[markdown]": {
        "editor.wordWrap": "wordWrapColumn",
        "editor.wrappingIndent": "none",
    },
    "editor.hover.enabled": false,
    "javascript.validate.enable": false,
    "editor.lightbulb.enabled": false
}

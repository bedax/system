                (tab-style automatic)
                (aux-info
                    (show-button-bar "true")))
            (dockable "gimp-tool-preset-editor"
                (tab-style automatic)
                (aux-info
                    (show-button-bar "true")
                    (edit-active "true")
                    (current-data "Crop"))))))
